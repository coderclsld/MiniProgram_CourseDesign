.goods-title{
  box-sizing: border-box;
  padding: 0 35rpx;
  font-size: 32rpx;
  line-height: 1.4;
  color: #000;
  margin-top: 28rpx;
  margin-bottom: 28rpx;
}
.goods-info-fx {
  position: absolute;
  right: 35rpx;
  top:660rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 130rpx;
}
.item {
  width: 50rpx;
  height: 50rpx;
  right: 130rpx;
  top: 100rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  background: #fff;
  z-index: 20;
} 
.xq{
  float: left;
  position: fixed;
  margin-bottom: 0px;
  left: 0rpx;
  bottom:0px;
  background:white;
  width:100%;
  height:40px;
  font-size: 24rpx;
  padding-left: 80px;
}